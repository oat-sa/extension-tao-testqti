            "interaction_choiceinteraction_59b693620237b969767169": {
                "serial": "interaction_choiceinteraction_59b693620237b969767169",
                "qtiClass": "choiceInteraction",
                "attributes": {
                    "responseIdentifier": "RESPONSE_2",
